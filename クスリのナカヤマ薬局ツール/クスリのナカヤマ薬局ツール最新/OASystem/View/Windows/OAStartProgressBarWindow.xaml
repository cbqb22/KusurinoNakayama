        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="clr-namespace:OASystem.View.UserControls"
        Title="ProgressBarWindow" 
        SizeToContent="WidthAndHeight" 
        WindowStyle="None" 
