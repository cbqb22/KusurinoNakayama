<UserControl x:Class="View.Core.TopPage.Tab.在庫管理.在庫管理TabItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
    xmlns:在庫検索="clr-namespace:View.Core.TopPage.Tab.在庫管理"
    xmlns:在庫更新="clr-namespace:View.Core.TopPage.Tab.在庫管理.在庫更新"
    Width="Auto" Height="Auto">
    <Grid x:Name="LayoutRoot" Background="White">
        <controls:TabControl>
            <controls:TabItem Header="在庫検索" x:Name="在庫検索TabItem" FontSize="12.667" FontWeight="Bold" Cursor="Hand">
                <在庫検索:在庫管理Frame/>
            </controls:TabItem>
            <controls:TabItem Header="在庫更新" x:Name="在庫更新TabItem" FontSize="12.667" FontWeight="Bold" Cursor="Hand">
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                    <在庫更新:在庫更新/>
                </ScrollViewer>
            </controls:TabItem>
        </controls:TabControl>

    </Grid>
</UserControl>
